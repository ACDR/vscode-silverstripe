{
	"if": {
        "prefix": "if",
        "body": [
            "<% if ${1} %>\n${2}\n<% end_if %>"
        ],
        "description": "if"
    },
    "loop": {
    	"prefix": "loop",
    	"body": [
    		"<% loop ${1} %>\n${2}\n<% end_loop %>"
    	]
    },
    "with": {
    	"prefix": "with",
    	"body": [
    		"<% with ${1} %>\n${2}\n<% end_with %>"
    	]
    },
    "ifelse": {
    	"prefix": "ifelse",
    	"body": [
    		"<% if ${1} %>\n${2}\n<% else %>\n${3}\n<% end_if %>"
    	]
    },
    "include": {
    	"prefix": "include",
    	"body": [
    		"<% include ${1:File} %>"
    	]
    },
    "control": {
    	"prefix": "control",
    	"body": [
    		"<% control ${1} %>\n${2}\n<% end_control %>"
    	]
    }
}
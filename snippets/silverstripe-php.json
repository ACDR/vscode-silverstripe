{
  "debug-message": {
    "prefix": "dm",
    "body": [
      "Debug::message(${1},);"
    ]
  },
  "Debug-show": {
    "prefix": "ds",
    "body": [
      "Debug::show(${1},);"
    ]
  },
  "HTMLEditorField": {
    "prefix": "htmlfield",
    "body": [
      "HtmlEditorField::create('${1:name},','${2:title},')$0"
    ]
  },
  "HTMLEditorFieldInTab": {
    "prefix": "htmlfieldtab",
    "body": [
      "\\$f->addFieldToTab('Root.${1:Content},', HtmlEditorField::create('${2:name},','${3:title},'))$0;\n"
    ]
  },
  "UploadField": {
    "prefix": "uploadfield",
    "body": [
      "UploadField::create('${1:name},','${2:title},'))$0;\n"
    ]
  },
  "UploadFieldInTab": {
    "prefix": "uploadfieldtab",
    "body": [
      "\\$f->addFieldToTab('Root.${1:Content},', UploadField::create('${2:name},','${3:title},'))$0;\n"
    ]
  },
  "TextField": {
    "prefix": "textfield",
    "body": [
      "TextField::create('${1:name},','${2:title},')$0"
    ]
  },
  "TextFieldInTab": {
    "prefix": "textfieldtab",
    "body": [
      "\\$f->addFieldToTab('Root.${1:Content},', TextField::create('${2:name},','${3:title},'))$0;\n"
    ]
  },
  "TextareaField": {
    "prefix": "textareafield",
    "body": [
      "TextareaField::create('${1:name},','${2:title},','${3:rows},','${4:cols},')$0"
    ]
  },
  "TextareaFieldInTab": {
    "prefix": "textareafieldtab",
    "body": [
      "\\$f->addFieldToTab('Root.${1:Content},', TextareaField::create('${2:name},','${3:title},'))$0;\n"
    ]
  },
  "addFieldToTab": {
    "prefix": "addfield",
    "body": [
      "addFieldToTab('${1:Root.Content},', $0,'${3:insertBefore},');\n"
    ]
  },
  "addFieldsToTab": {
    "prefix": "addfields",
    "body": [
      "addFieldsToTab('${1:Root.Content},', array(\n\t$0\n));\n"
    ]
  }
}